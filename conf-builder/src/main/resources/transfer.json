{
    "logLevel": "${log.level}",
    "debug": ${log.debug},
    "minStep": 30,
    "http": {
        "enabled": true,
        "listen": "0.0.0.0:${port.http.transfer}"
    },
    "rpc": {
        "enabled": true,
        "listen": "0.0.0.0:${port.rpc.transfer}"
    },
    "socket": {
        "enabled": false,
        "listen": "0.0.0.0:4444",
        "timeout": 3600
    },
    "judge": {
        "enabled": true,
        "batch": 400,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "replicas": 500,
        "cluster": {
            ${m.transfer.cluster.judge}
        }
    },
    "graph": {
        "enabled": true,
        "batch": 400,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "replicas": 500,
        "cluster": {
            ${cluster.graph}
        }
    },
    "tsdb": {
        "enabled": false,
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "retry": 3,
        "address": "127.0.0.1:8088"
    },
    "influxdb": {
        "enabled": false,
        "batch": 600,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "addr": "owl_transfer:rpvLuwRhHAGhA8Rd@http(influxdata.owl.fastweb.com.cn:80)/owl"
    },
    "nqmRest": {
        "enabled":${m.transfer.nqm.enable},
        "batch": 200,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "fping": "${cassandra.conn}/nqm/icmp",
        "tcpping": "${cassandra.conn}/nqm/tcp",
        "tcpconn": "${cassandra.conn}/nqm/tcpconn"
    },
    "staging": {
        "enabled": ${m.transfer.staging.enable},
        "batch": 400,
        "connTimeout": 1000,
        "callTimeout": 5000,
        "maxConns": 32,
        "maxIdle": 32,
        "retry": 3,
        "address": "${m.transfer.staging.addr}",
        "filters": [
            ${m.transfer.staging.filters}
        ]
    }
}
